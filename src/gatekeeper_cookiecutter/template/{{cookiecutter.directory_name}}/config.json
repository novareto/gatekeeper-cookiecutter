{
    "db": {
        "uri": "sqlite:////{{cookiecutter.fullpath}}/{{cookiecutter.directory_name}}/var/admin.db",
        "key": "admin_db"
    },
        "global": {
            "domain": "{{cookiecutter.domain}}",
            "admin": "{{cookiecutter.ServerAdmin}}",
            "dest": "{{cookiecutter.uri}}",
            "uri": "{{cookiecutter.uri}}", 
            "ip": "{{cookiecutter.public_ip}}",
            "user_gruppe": "{{cookiecutter.deamon_user}}"
        },
    "session": {
        "path": "{{cookiecutter.fullpath}}/{{cookiecutter.directory_name}}/var/cache/sessions",
        "timeout": "300"
    },
    "crypto": {
        "pubkey": "{{cookiecutter.fullpath}}/{{cookiecutter.directory_name}}/etc/pubkey.pem",
        "privkey": "{{cookiecutter.fullpath}}/{{cookiecutter.directory_name}}/etc/privkey.pem",
        "cipher": "{{cookiecutter.secret}}"
    }
}
